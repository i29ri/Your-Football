<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>チーム名</th>
      <th>アイコン</th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= link_to team_path(team) do %>
          <%= team.name %>
        <% end %></td>
        <td><image src=<%= team.image %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= image_tag "kawasaki-image.png"%>

