<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>ユーザーID</th>
      <th>ニックネーム</th>
      <th>アイコン</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.yourfoot_ID %></td>
        <td><%= user.nickname %></td>
        <td>  <%= attachment_image_tag user, :profile_image, :fill, 100, 100, fallback: "noimage.jpg"%></td>
      </tr>
    <% end %>
  </tbody>
</table>